{
	"colorize_brackets": false,
	"show_whitespaces": "selection",
	"ui_font_family": ".SystemUIFont",
	"buffer_font_size": 14.0,
	"ui_font_size": 13.0,
	"buffer_font_weight": 700.0,
	"buffer_font_family": "Victor Mono",
	"search": {
		"include_ignored": false,
	},
	"indent_guides": {
		"line_width": 1,
		"active_line_width": 1,
		"background_coloring": "disabled",
		"coloring": "fixed",
		"enabled": true,
	},
	"languages": {
		"Rust": {
			"inlay_hints": {
				"show_background": false,
			},
		},
		"Lua": {
			"format_on_save": "on",
			"formatter": {
				"external": {
					"command": "stylua",
					"arguments": [
						"--syntax=CfxLua",
						"--config-path",
						"/Users/vipex/stylua.toml",
						"--respect-ignores",
						"--stdin-filepath",
						"{buffer_path}",
						"-",
					],
				},
			},
		},
	},
	"zoomed_padding": true,
	"active_pane_modifiers": {
		"border_size": 0.0,
		"inactive_opacity": 1.0,
	},
	"experimental.theme_overrides": {
		"syntax": {
			"comment": {
				"font_style": "italic",
			},
			"comment.doc": {
				"font_style": "italic",
			},
			"keyword": {
				"font_style": "italic",
				"font_weight": null,
			},
			"predictive": {
				"font_style": "italic",
				"font_weight": null,
			},
		},
	},
	"relative_line_numbers": "wrapped",
	"title_bar": {
		"show_menus": false,
		"show_sign_in": true,
		"show_user_picture": false,
		"show_onboarding_banner": true,
		"show_project_items": true,
		"show_branch_name": true,
		"show_branch_icon": false,
	},
	"status_bar": {
		"experimental.show": false,
	},
	"toolbar": {
		"agent_review": false,
		"breadcrumbs": false,
		"code_actions": false,
		"quick_actions": false,
		"selections_menu": false,
	},
	"linked_edits": false,
	"jsx_tag_auto_close": {
		"enabled": false,
	},
	"use_auto_surround": true,
	"cursor_shape": "hollow",
	"lsp_document_colors": "none",
	"show_completion_documentation": false,
	"file_scan_exclusions": [],
	"preview_tabs": {
		"enable_preview_from_file_finder": false,
	},
	"tabs": {
		"show_close_button": "hover",
		"show_diagnostics": "off",
		"close_position": "left",
		"file_icons": false,
		"git_status": true,
	},
	"icon_theme": {
		"dark": "Modern Icons (Dark)",
		"light": "Modern Icons (Light)",
	},
	"collaboration_panel": {
		"dock": "right",
	},
	"outline_panel": {
		"folder_icons": false,
		"file_icons": false,
		"dock": "right",
	},
	"git_panel": {
		"dock": "left",
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false,
	},
	"debugger": {
		"dock": "right",
	},
	"max_tabs": 5,
	"inline_code_actions": false,
	"use_smartcase_search": true,
	// "disable_ai": true,
	"vim_mode": true,
	"use_autoclose": true,
	"buffer_line_height": {
		"custom": 1.2999999523162842,
	},
	"ui_font_weight": 600.0,
	"use_system_window_tabs": true,
	"hard_tabs": true,
	"hover_popover_enabled": true,
	"extend_comment_on_newline": false,
	"edit_predictions": {
		"enabled_in_text_threads": false,
	},
	"gutter": {
		"runnables": false,
		"min_line_number_digits": 0,
		"breakpoints": false,
		"line_numbers": true,
		"folds": false,
	},
	"file_finder": {
		"file_icons": true,
		"include_ignored": "smart",
		"modal_max_width": "medium",
	},
	"agent": {
		"dock": "left",
		"default_width": 640.0,
		"default_model": {
			"provider": "anthropic",
			"model": "claude-sonnet-4-5-latest",
		},
		"default_profile": "minimal",
		"enabled": true,
		"play_sound_when_agent_done": true,
	},
	"inlay_hints": {
		"show_background": true,
		"show_value_hints": true,
		"enabled": false,
		"show_parameter_hints": true,
		"show_type_hints": true,
	},
	"remove_trailing_whitespace_on_save": true,
	"format_on_save": "on",
	"ensure_final_newline_on_save": true,
	"buffer_font_features": {
		"calt": false,
	},
	"features": {
		"edit_prediction_provider": "none",
	},
	"disable_ai": false,
	"vim": {
		"toggle_relative_line_numbers": true,
		"highlight_on_yank_duration": 200,
		"use_system_clipboard": "always",
		"use_smartcase_find": true,
	},
	"tab_size": 4,
	"tab_bar": {
		"show_pinned_tabs_in_separate_row": false,
		"show_tab_bar_buttons": false,
		"show_nav_history_buttons": false,
		"show": false,
	},
	"terminal": {
		"alternate_scroll": "on",
		"blinking": "on",
		"font_weight": 700,
		"toolbar": {
			"breadcrumbs": false,
		},
		"line_height": "standard",
		"dock": "bottom",
		"scrollbar": {
			"show": "never",
		},
	},
	"project_panel": {
		"indent_size": 10.0,
		"hide_root": false,
		"starts_open": false,
		"auto_reveal_entries": true,
		"git_status": true,
		"folder_icons": true,
		"indent_guides": {
			"show": "never",
		},
		"file_icons": true,
		"entry_spacing": "comfortable",
		"hide_gitignore": false,
		"default_width": 200.0,
		"dock": "right",
		"auto_fold_dirs": false,
		"sticky_scroll": true,
		"scrollbar": {
			"show": "never",
		},
	},
	"scrollbar": {
		"axes": {
			"vertical": true,
			"horizontal": false,
		},
		"show": "never",
	},
	"lsp": {
		"rust-analyzer": {
			"initialization_options": {
				"diagnostics": {
					"experimental": {
						"enable": true,
					},
				},
				"checkOnSave": false,
				"cargo": {
					"allTargets": false,
				},
				"check": {
					"workspace": false,
				},
			},
		},
		"tailwindcss-language-server": {
			"settings": {
				"classFunctions": ["cva", "cx"],
				"experimental": {
					"classRegex": [
						"[cls|className]\\s\\:\\=\\s\"([^\"]*)",
						"class:\\s*?[\"'`]([^\"'`]*).*?,",
					],
				},
			},
		},
	},
	"diagnostics": {
		"include_warnings": true,
		"inline": {
			"enabled": true,
			"padding": 1,
		},
	},
	"theme": {
		"mode": "dark",
		"light": "Siri Light",
		"dark": "JetBrains Dark",
	},
}
