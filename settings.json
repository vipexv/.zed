{
	"icon_theme": "Zed (Default)",
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"use_smartcase_search": true,
	"vim_mode": true,
	"use_autoclose": true,
	"ui_font_family": "SF Pro Text",
	"buffer_font_family": "SF Mono",
	"ui_font_size": 18,
	"ui_font_weight": 400,
	"buffer_font_size": 20,
	"preferred_line_length": 125,
	"soft_wrap": "preferred_line_length",
	"buffer_line_height": "standard",
	"hard_tabs": true,
	"hover_popover_enabled": false,
	"extend_comment_on_newline": false,
	"edit_predictions": {
		"enabled_in_text_threads": false
	},
	"gutter": {
		"line_numbers": true,
		"folds": false
	},
	"file_finder": {
		"modal_max_width": "medium"
	},
	"features": {
		"edit_prediction_provider": "none"
	},
	"agent": {
		"enabled": false,
		"version": "1"
	},
	"inlay_hints": {
		"enabled": false,
		"show_parameter_hints": false,
		"show_type_hints": false
	},
	"vim": {
		"toggle_relative_line_numbers": true,
		"highlight_on_yank_duration": 200,
		"use_system_clipboard": "always"
	},
	"diagnostics": {
		"include_warnings": true,
		"inline": {
			"enabled": true,
			"padding": 1
		}
	},
	"tab_size": 4,
	"tab_bar": {
		"show": false
	},
	"terminal": {
		"shell": {
			"program": "C:\\Program Files\\Git\\bin\\bash.exe"
		},
		"toolbar": {
			"breadcrumbs": false
		},
		"line_height": "standard",
		"dock": "bottom",
		"scrollbar": {
			"show": "never"
		},
		"font_size": 16
	},
	"project_panel": {
		"default_width": 240,
		"indent_size": 15,
		"dock": "right",
		"scrollbar": {
			"show": "never"
		}
	},
	"scrollbar": {
		"show": "never"
	},
	"lsp": {
		"rust-analyzer": {
			"initialization_options": {
				"diagnostics": {
					"experimental": {
						"enable": true
					}
				},
				"checkOnSave": false,
				"cargo": {
					"allTargets": false
				},
				"check": {
					"workspace": false
				}
			}
		}
	},
	"theme": {
		"mode": "system",
		"light": "Catppuccin Latte",
		"dark": "VSCode Dark Modern"
	}
	// "experimental.theme_overrides": {
	// 	"editor.background": "#090909",
	// 	"status_bar.background": "#090909",
	// 	"toolbar.background": "#090909",
	// 	"panel.background": "#090909",
	// 	"editor.gutter.background": "#090909",
	// 	"terminal.background": "#090909",
	// 	"title_bar.background": "#090909"
	// },
}
