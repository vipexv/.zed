{
	"git_panel": {
		"dock": "left"
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"debugger": {
		"dock": "left"
	},
	"toolbar": {
		"selections_menu": false,
		// "breadcrumbs": false,
		"agent_review": false,
		"code_actions": false,
		"quick_actions": false
	},
	"use_smartcase_search": true,
	"vim_mode": true,
	"use_autoclose": true,
	"use_system_path_prompts": false,
	"soft_wrap": "none",
	"buffer_font_family": "Geist Mono",
	"ui_font_family": "SF Pro Text",
	"buffer_line_height": "standard",
	"hard_tabs": true,
	"hover_popover_enabled": false,
	"extend_comment_on_newline": false,
	"auto_signature_help": true,
	"max_tabs": null,
	"buffer_font_size": 16,
	"buffer_font_weight": 500,
	"ui_font_weight": 500,
	"edit_predictions": {
		"enabled_in_text_threads": false
	},
	"gutter": {
		"line_numbers": true,
		"folds": false
	},
	"file_finder": {
		"modal_max_width": "medium"
	},
	"features": {
		"edit_prediction_provider": "none"
	},
	"agent": {
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-3-5-sonnet"
		},
		"default_profile": "write",
		"enabled": true,
		"play_sound_when_agent_done": true
	},
	"inlay_hints": {
		"enabled": false,
		"show_parameter_hints": false,
		"show_type_hints": false
	},
	"remove_trailing_whitespace_on_save": true,
	"ensure_final_newline_on_save": true,
	"vim": {
		"toggle_relative_line_numbers": true,
		"highlight_on_yank_duration": 200,
		"use_system_clipboard": "always",
		"use_smartcase_find": true
	},
	"tab_size": 4,
	"tab_bar": {
		"show": false
	},
	"terminal": {
		"shell": {
			"program": "C:\\Users\\v\\scoop\\apps\\git\\current\\bin\\bash.exe"
		},
		"default_height": 2080,
		"toolbar": {
			"breadcrumbs": false
		},
		"line_height": "standard",
		"dock": "bottom",
		"scrollbar": {
			"show": "never"
		},
		"font_size": 14
	},
	"project_panel": {
		"default_width": 340,
		"indent_size": 15,
		"dock": "left",
		"auto_fold_dirs": false,
		"scrollbar": {
			"show": "never"
		}
	},
	"scrollbar": {
		"show": "never"
	},
	"lsp": {
		"rust-analyzer": {
			"initialization_options": {
				"diagnostics": {
					"experimental": {
						"enable": false
					}
				},
				"checkOnSave": true,
				"cargo": {
					"allTargets": false
				},
				"check": {
					"workspace": false
				}
			}
		}
	},
	"diagnostics": {
		"cargo": {
			"fetch_cargo_diagnostics": false
		},
		"include_warnings": true,
		"inline": {
			"enabled": true,
			"padding": 1
		}
	},
	"theme": {
		"mode": "system",
		"light": "One Light",
		"dark": "JetBrains Rider Dark"
	},
	"icon_theme": {
		"dark": "JetBrains New UI Icons (Dark)",
		"light": "JetBrains New UI Icons (Light)",
		"mode": "system"
	}
	// "experimental.theme_overrides": {
	// 	"editor.background": "#090909",
	// 	"status_bar.background": "#090909",
	// 	"toolbar.background": "#090909",
	// 	"panel.background": "#090909",
	// 	"editor.gutter.background": "#090909",
	// 	"terminal.background": "#090909",
	// 	"title_bar.background": "#090909"
	// }
}
