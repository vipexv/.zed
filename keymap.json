[
	{
		"context": "vim_mode == visual",
		"bindings": {
			"shift-s": ["vim::PushAddSurrounds", {}],
		},
	},
	{
		"context": "vim_mode == insert",
		"bindings": {
			"cmd-s": ["workspace::SendKeystrokes", "escape : w enter"],
		},
	},
	{
		"context": "Terminal",
		"bindings": {
			"ctrl-`": "workspace::ToggleBottomDock",
			"ctrl-shift-/": "workspace::NewTerminal",
		},
	},
	{
		"context": "vim_mode == normal || EmptyPane",
		"bindings": {
			"space p v": "workspace::ToggleLeftDock",
			"space p r": "workspace::ToggleRightDock",
			"space p l": "workspace::ToggleLeftDock",
			"space space": "tab_switcher::Toggle",
			"space g": "go_to_line::Toggle",
		},
	},
	{
		"context": "ProjectPanel && !editing",
		"bindings": {
			"space p v": "workspace::ToggleLeftDock",
			"c a": "project_panel::CollapseAllEntries",
			"c c": "project_panel::CollapseSelectedEntry",
		},
	},
	{
		"context": "AgentPanel",
		"bindings": {
			"cmd-?": "workspace::ToggleLeftDock",
		},
	},
	{
		"context": "Workspace && vim_mode == normal || vim_mode == normal || ProjectPanel && !editing || EmptyPane",
		"bindings": {
			"f f": "file_finder::Toggle",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"cmd-r": null,
		},
	},
	{
		"context": "Editor || Terminal",
		"bindings": {
			"cmd-}": "window::ShowNextWindowTab",
			"cmd-{": "window::ShowPreviousWindowTab",
		},
	},
]
