{
	"$schema": "https://zed.dev/schema/themes/v0.1.0.json",
	"name": "Tsoding Dark Theme",
	"author": "Ported from VSCode",
	"themes": [
		{
			"name": "Tsoding Dark Theme",
			"appearance": "dark",
			"style": {
				"background": "#181818",
				"elevated_surface.background": "#2d2d2d",
				"surface.background": "#181818",
				"border": "#212121",
				"text": "#e8e8e8",
				"text.muted": "#919191",
				"icon": "#fff7c7",
				"icon.muted": "#fffff9",
				"element.background": "#181818",
				"element.hover": "#7575753a",
				"element.selected": "#d38f8f32",
				"status_bar.background": "#282828",
				"title_bar.background": "#212121",
				"tab_bar.background": "#2e2e2e",
				"tab.inactive_background": "#2e2e2e",
				"tab.active_background": "#181818",
				"editor.foreground": "#e8e8e8",
				"editor.background": "#181818",
				"editor.gutter.background": "#181818",
				"editor.active_line.background": "#181818",
				"editor.line_number": "#555555",
				"editor.active_line_number": "#afadad",
				"search.match_background": "#d38f8f32",
				"panel.background": "#282828",
				"border.focused": "#d6b69a",
				"border.selected": "#d38f8f32",
				"terminal.background": "#181818",
				"terminal.ansi.black": "#181818",
				"terminal.ansi.red": "#c22121",
				"terminal.ansi.green": "#79bf46",
				"terminal.ansi.yellow": "#ffdd33",
				"terminal.ansi.blue": "#6796E6",
				"terminal.ansi.magenta": "#ff0000",
				"terminal.ansi.cyan": "#BFC5BF",
				"terminal.ansi.white": "#e8e8e8",
				"syntax": {
					"primary": { "color": "#e8e8e8" },
					"comment": { "color": "#ff9b21", "font_style": "normal" },
					"comment.doc": { "color": "#ff9b21" },

					"keyword": { "color": "#ffdd33" },
					"keyword.control": { "color": "#ffdd33" },
					"keyword.function": { "color": "#ffdd33" },
					"keyword.operator.word": { "color": "#ffdd33" },
					"storage": { "color": "#ffdd33" },
					"storage.type": { "color": "#ffdd33" },
					"storage.modifier": { "color": "#ffdd33" },

					"string": { "color": "#79bf46" },
					"string.quoted": { "color": "#79bf46" },
					"string.template": { "color": "#79bf46" },

					"type.builtin": { "color": "#ffdd33" },
					"boolean": { "color": "#ffdd33" },
					"constant.language": { "color": "#ffdd33" },

					"variable": { "color": "#e8e8e8" },
					"variable.special": { "color": "#e8e8e8" },
					"function": { "color": "#e8e8e8" },
					"function.method": { "color": "#e8e8e8" },
					"type": { "color": "#e8e8e8" },
					"constructor": { "color": "#e8e8e8" },
					"property": { "color": "#e8e8e8" },
					"attribute": { "color": "#e8e8e8" },
					"label": { "color": "#e8e8e8" },

					"number": { "color": "#BFC5BF" },
					"constant": { "color": "#BFC5BF" },
					"operator": { "color": "#BFC5BF" },
					"punctuation": { "color": "#BFC5BF" },
					"punctuation.bracket": { "color": "#BFC5BF" },
					"punctuation.delimiter": { "color": "#BFC5BF" },
					"tag": { "color": "#e8e8e8" }
				}
			}
		}
	],
	"id": "tsoding-dark-theme-v2"
}
